#include "main.h"
/**
 *print_sign - entry point 
 *@n : define the sign 
 *description : prints +1 for positive value , 0 for zero otherwise -1
 *
 *Return:0 always for succes operation
 */
int print_sign(int n)
{
	if (n > 0)
	{
		_putchar('+');
		return (1);
	}
	else if (n < 0)
	{
		_putchar('-');
		return (-1);
	}
	else
	{
		_putchar('0');
		return (0);
	}
}
